{
    "manifest_version": 3,
    "name": "AudioBook API Caller",
    "version": "1.0",
    "permissions": ["activeTab", "scripting","tabs"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["https://www.navaar.ir/*"],
            "js": ["content.js"]
        }
    ]
}
